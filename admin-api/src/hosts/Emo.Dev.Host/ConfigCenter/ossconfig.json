{
  "OssConfig": {
    //本地上传配置
    "LocalUploadConfig": {
      //上传目录
      "Directory": "upload",
      //日期目录
      "DateTimeDirectory": "yyyy/MM/dd",
      "Md5": false,
      //文件最大大小
      "MaxSize": 104857600,
      //包含文件拓展名列表
      "IncludeExtension": [],
      //排除文件拓展名列表
      "ExcludeExtension": [ ".exe", ".dll", ".jar" ]
    },
    //文件存储供应商
    "Provider": "Minio",
    //OSS配置列表
    "OSSConfigs": [
      //Minio
      {
        "Provider": "Minio",
        "Endpoint": "127.0.0.1:9006",
        "Region": "",
        "AccessKey": "minio",
        "SecretKey": "minio",
        "IsEnableHttps": false,
        "IsEnableCache": true,
        "BucketName": "admin",
        "Url": "", //文件外链
        "Md5": false,
        "Enable": false
      },
      //阿里云
